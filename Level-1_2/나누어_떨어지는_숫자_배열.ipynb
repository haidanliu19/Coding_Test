      "source": [
        "def solution(arr, divisor):\n",
        "    answer = sorted(filter(lambda x: not (x% divisor), arr))\n",
        "    return answer or [-1]\n",
        "\n",
        "arr = [5, 9, 7, 10]\n",
        "divisor = 5\n",
        "solution(arr, divisor)"
