      "source": [
        "def solution(s):\n",
        "    answer = \"\"\n",
        "    for i in s.split(\" \"):\n",
        "        s_ = \"\"\n",
        "        for j in range(len(i)):\n",
        "            s_ += i[j].lower() if j %2 else i[j].upper()\n",
        "        answer += s_ + \" \"\n",
        "    return answer[:-1]\n",
        "\n",
        "s = \"try hello world\"\n",
        "solution(s)"
      ]
