      "source": [
        "def solution(arr):\n",
        "    answer = [arr[0]]\n",
        "    # [실행] 버튼을 누르면 출력 값을 볼 수 있습니다.\n",
        "    for i in arr[1:]:\n",
        "        if i != answer[-1]:\n",
        "            answer.append(i)\n",
        "    return answer\n",
        "\n",
        "arr = [1,1,3,3,0,1,1]\n",
        "solution(arr)"
      ]
