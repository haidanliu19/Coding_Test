      "source": [
        "def solution(arr):\n",
        "    answer = []\n",
        "    answer.append(arr[0])\n",
        "    for idx, val in enumerate(arr):\n",
        "        if answer[-1] != val:\n",
        "            answer.append(val)\n",
        "    return answer\n",
        "\n",
        "arr = [1,1,3,3,0,1,1]\n",
        "solution(arr)"
      ],
