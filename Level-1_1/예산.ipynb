      "cell_type": "code",
      "source": [
        "def solution(d, budget):\n",
        "    answer = 0\n",
        "    d.sort()\n",
        "    while sum(d) > budget:\n",
        "        d.pop()\n",
        "    answer = len(d)\n",
        "    return answer\n",
        "\n",
        "d = [1, 3, 2, 5, 4]\n",
        "budget = 9\n",
        "solution(d, budget)"
      ],
