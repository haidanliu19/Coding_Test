        "from itertools import combinations\n",
        "def solution(numbers):\n",
        "    answer = []\n",
        "    answer = sorted(list(set(map(sum, list(combinations(numbers, 2))))))\n",
        "    return answer\n",
        "\n",
        "\n",
        "numbers = [2,1,3,4,1]\n",
        "solution(numbers)"
      ],
