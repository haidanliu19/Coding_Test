        "import datetime\n",
        "def solution(a, b):\n",
        "    week_list = ['MON', 'TUE', 'WED', 'THU', 'FRI', 'SAT', 'SUN']\n",
        "    answer = week_list[datetime.date(2016, a, b).weekday()]\n",
        "    return answer\n",
        "\n",
        "\n",
        "a = 5\n",
        "b = 24\n",
        "solution(a, b)"
