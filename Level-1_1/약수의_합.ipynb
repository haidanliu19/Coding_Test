      "source": [
        "def solution(n):\n",
        "    answer = [n]\n",
        "    answer += [i  for i in range(1, n //2 +1) if n % i == 0 ]\n",
        "    return sum(answer)\n",
        "\n",
        "n = 16\n",
        "solution(n)"
      ],
