      "source": [
        "def solution(s):\n",
        "    len_s = len(s)//2\n",
        "    answer = s[len_s -1 : len_s +1] if len(s)%2 == 0 else s[len_s]\n",
        "    return answer\n",
        "\n",
        "s = \"qwer\"\n",
        "solution(s)"
