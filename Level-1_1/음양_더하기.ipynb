      "source": [
        "def solution(absolutes, signs):\n",
        "    answer = sum([ -absolute  if not sign else absolute for absolute, sign  in zip(absolutes, signs)])\n",
        "    return answer\n",
        "\n",
        "absolutes = [4,7,12]\n",
        "signs = [True,False,True]\n",
        "solution(absolutes, signs)"
      ],
