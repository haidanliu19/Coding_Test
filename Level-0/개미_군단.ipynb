      "source": [
        "def solution(hp):\n",
        "    answer = 0\n",
        "    for i in [5, 3, 1]:\n",
        "        div, mod = divmod(hp, i)\n",
        "        answer += div\n",
        "        hp = mod\n",
        "    return answer\n",
        "\n",
        "hp = 23\n",
        "solution(hp)"
      ],
