      "source": [
        "def solution(numbers):\n",
        "    answer = 0\n",
        "    numbers.sort()\n",
        "    num_left = numbers[0] * numbers[1]\n",
        "    num_right = numbers[-1] * numbers[-2]\n",
        "    answer = num_left if num_left > num_right else num_right\n",
        "    return answer\n",
        "\n",
        "numbers = [1, 2, -3, 4, -5]\n",
        "solution(numbers)"
      ],
