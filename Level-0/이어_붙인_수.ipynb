      "source": [
        "def solution(num_list):\n",
        "    answer = int(\"\".join(map(str, filter(lambda x: x% 2, num_list)))) + int(\"\".join(map(str, filter(lambda x: x% 2 == 0, num_list))))\n",
        "    return answer\n",
        "\n",
        "num_list = [3, 4, 5, 2, 1]\n",
        "solution(num_list)"
