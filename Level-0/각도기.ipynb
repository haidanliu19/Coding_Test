        "def solution(angle):\n",
        "    answer = -1\n",
        "    if 0 < angle < 90:\n",
        "        answer = 1\n",
        "    elif angle == 90:\n",
        "        answer = 2\n",
        "    elif 90 < angle < 180:\n",
        "        answer = 3\n",
        "    elif angle == 180:\n",
        "        answer = 4\n",
        "    return answer\n",
        "\n",
        "angle = 70\n",
        "solution(angle)"
      ]
