      "source": [
        "def solution(str_list, ex):\n",
        "    answer = \"\".join(['' if ex in i else i for i in str_list ])\n",
        "    return answer\n",
        "\n",
        "str_list = [\"abc\", \"def\", \"ghi\"]\n",
        "ex = \"ef\"\n",
        "solution(str_list, ex)"
