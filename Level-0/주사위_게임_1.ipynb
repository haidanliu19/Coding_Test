      "cell_type": "code",
      "source": [
        "def solution(a, b):\n",
        "    answer = 0\n",
        "    if (a % 2 != 0) and (b % 2 != 0):\n",
        "        answer = a ** 2 + b ** 2\n",
        "    elif (a % 2 == 0) and (b % 2 ==0):\n",
        "        answer = abs(a-b)\n",
        "    else:\n",
        "        answer = 2 * (a + b)\n",
        "    return answer\n",
        "\n",
        "a = 3\n",
        "b = 5\n",
        "solution(a, b)"
      ],
