        "def solution(age):\n",
        "    answer = ''\n",
        "    dic = {}\n",
        "    for i in range(ord('z')- ord('a') + 1):\n",
        "        dic[str(i)] = chr(i+ 97)\n",
        "    answer = \"\".join([dic.get(i) for i in str(age)])\n",
        "    \n",
        "    return answer\n",
        "\n",
        "age= 23\n",
        "solution(age)"
