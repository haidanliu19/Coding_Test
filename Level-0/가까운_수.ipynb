      "source": [
        "def solution(array, n):\n",
        "    array_1 = [(i, abs(array[i] - n), array[i]) for i in range(len(array))]\n",
        "    answer = sorted(array_1, key = lambda x: (x[1],x[2]))[0][2]\n",
        "    return answer\n",
        "\n",
        "\n",
        "array = [3, 10, 28]\n",
        "n = 20\n",
        "solution(array, n)"
      ],
