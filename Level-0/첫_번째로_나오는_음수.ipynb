        "def solution(num_list):\n",
        "    answer = -1\n",
        "    for i, val in enumerate(num_list):\n",
        "        if val < 0:\n",
        "            answer = i\n",
        "            break\n",
        "    return answer\n",
        "\n",
        "num_list = [12, 4, 15, 46, 38, -2, 15]\n",
