      "source": [
        "def solution(arr, query):\n",
        "    answer = arr\n",
        "    for i, val in enumerate(query):\n",
        "        if i % 2 == 0: # 짝수\n",
        "            answer = answer[: val+1]\n",
        "        else: # 홀수\n",
        "            answer = answer[val:]\n",
        "    return answer\n",
        "\n",
        "arr = [0, 1, 2, 3, 4, 5]\n",
        "query = [4, 1, 2]\n",
        "solution(arr, query)"
      ]
