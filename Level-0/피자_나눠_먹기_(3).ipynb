      "source": [
        "def solution(slice, n):\n",
        "    answer = 0\n",
        "    div, mod = divmod(n, slice)\n",
        "    answer = div\n",
        "    if mod > 0:\n",
        "        answer += 1\n",
        "    return answer\n",
        "\n",
        "slice = 7\n",
        "n = 10\n",
        "solution(slice, n)"
      ],
