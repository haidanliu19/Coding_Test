      "source": [
        "def solution(num_list):\n",
        "    answer = 1\n",
        "    if len(num_list)> 10:\n",
        "        answer = sum(num_list)\n",
        "    else:\n",
        "        for i in num_list:\n",
        "            answer *= i\n",
        "    return answer\n",
        "\n",
        "num_list = [3, 4, 5, 2, 5, 4, 6, 7, 3, 7, 2, 2, 1]\n",
        "solution(num_list)"
      ]
