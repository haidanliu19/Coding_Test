      "cell_type": "code",
      "source": [
        "def solution(box, n):\n",
        "    answer = 1\n",
        "    for i in box:\n",
        "        answer *= (i // n)\n",
        "    return answer\n",
        "\n",
        "box = [1, 1, 1]\n",
        "n = 1\n",
        "solution(box, n)"
      ],
