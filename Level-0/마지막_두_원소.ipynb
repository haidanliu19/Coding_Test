        "def solution(num_list):\n",
        "    answer = num_list\n",
        "    num = num_list[-1] - num_list[-2]\n",
        "    if num <= 0:\n",
        "        num = num_list[-1] * 2\n",
        "    answer.append( num )\n",
        "    return answer\n",
        "\n",
        "num_list = [2, 1, 6]\n",
        "solution(num_list)"
