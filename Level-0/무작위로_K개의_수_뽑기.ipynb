      "source": [
        "def solution(arr, k):\n",
        "    answer = []\n",
        "    for i in arr:\n",
        "        if i not in answer:\n",
        "            answer.append(i)\n",
        "    answer = answer[:k]\n",
        "    answer += (k - len(answer) ) * [-1]\n",
        "    return answer\n",
        "\n",
        "\n",
        "arr =  [0, 1, 0, 1]\n",
        "k = 4\n",
        "solution(arr, k)"
      ]
