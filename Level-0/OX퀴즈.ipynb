      "source": [
        "def solution(quiz):\n",
        "    answer = []\n",
        "    for i in quiz:\n",
        "        if eval(i.split(\"=\")[0]) == int(i.split(\"=\")[1]):\n",
        "            answer.append(\"O\")\n",
        "        else:\n",
        "            answer.append(\"X\")\n",
        "    return answer\n",
        "\n",
        "quiz = [\"3 - 4 = -3\", \"5 + 6 = 11\"]\n",
        "solution(quiz)"
      ]
    }
  ]
