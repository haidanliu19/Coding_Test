      "source": [
        "def solution(numbers, n):\n",
        "    answer = 0\n",
        "    for number in numbers:\n",
        "        if answer > n:\n",
        "            break\n",
        "        answer += number\n",
        "    return answer\n",
        "\n",
        "numbers = [34, 5, 71, 29, 100, 34]\n",
        "n = 123\n",
        "solution(numbers, n)"
      ]
