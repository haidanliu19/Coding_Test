        "\n",
        "def solution(before, after):\n",
        "    answer = 0\n",
        "    for i in before:\n",
        "        if i in after:\n",
        "            after = after.replace(i, '', 1)\n",
        "    answer = 1 if len(after) == 0 else 0\n",
        "    return answer\n",
        "\n",
        "before = \"olleh\"\n",
        "after = \"hello\"\n",
        "solution(before, after)"
      ],
