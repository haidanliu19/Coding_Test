        "def solution(a, d, included):\n",
        "    answer = sum([a + d * i for i in range(len(included)) if included[i]])\n",
        "    return answer\n",
        "\n",
        "a = 3\n",
        "d = 4\n",
        "included = [True, False, False, True, True]\n",
        "solution(a, d, included)"
