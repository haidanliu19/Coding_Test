      "source": [
        "def solution(arr, queries):\n",
        "    answer = []\n",
        "    for i in queries:\n",
        "        s, e = i[0], i[1]\n",
        "        for i in range(s, e + 1):\n",
        "            arr[i] += 1\n",
        "    answer = arr\n",
        "    return answer\n",
        "\n",
        "arr = [0, 1, 2, 3, 4]\n",
        "queries = [[0, 1],[1, 2],[2, 3]]\n",
        "solution(arr, queries)"
      ]
