      "cell_type": "code",
      "source": [
        "def solution(dots):\n",
        "    answer = 0\n",
        "    widths = [i[0] for i in dots]\n",
        "    heights =  [i[1] for i in dots]\n",
        "    answer = (max(widths) - min(widths)) * (max(heights) - min(heights))\n",
        "    return answer\n",
        "\n",
        "dots = [[1, 1], [2, 1], [2, 2], [1, 2]]\n",
        "solution(dots)"
      ],
