        "def solution(my_string, indices):\n",
        "    answer = my_string\n",
        "    indices.sort(reverse = True)\n",
        "    for i in indices:\n",
        "        answer = answer[:i] + answer[i+1:]\n",
        "    return answer\n",
        "\n",
        "my_string = \"apporoograpemmemprs\"\n",
        "indices = [1, 16, 6, 15, 0, 10, 11, 3]\n",
        "solution(my_string, indices)"
