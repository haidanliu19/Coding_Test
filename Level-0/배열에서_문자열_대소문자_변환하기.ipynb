      "source": [
        "def solution(strArr):\n",
        "    answer = []\n",
        "    for i, val in enumerate(strArr):\n",
        "        if i % 2 == 0:\n",
        "            answer.append(val.lower())\n",
        "        else:\n",
        "            answer.append(val.upper())\n",
        "    return answer\n",
        "\n",
        "strArr = [\"AAA\",\"BBB\",\"CCC\",\"DDD\"]\n",
        "solution(strArr)"
      ]
