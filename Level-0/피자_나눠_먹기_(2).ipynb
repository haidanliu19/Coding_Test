      "source": [
        "def solution(n):\n",
        "    answer = 0\n",
        "    for i in range(max(n, 6), n * 6 + 1):\n",
        "         if i % n == 0 and i % 6 == 0:\n",
        "             answer = i\n",
        "             break\n",
        "    answer //= 6\n",
        "    return answer\n",
        "\n",
        "\n",
        "n = 10\n",
        "solution(n)"
      ],
