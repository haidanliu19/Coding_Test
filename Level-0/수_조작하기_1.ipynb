        "def solution(n, control):\n",
        "    answer = n\n",
        "    dict_ = {\"w\": 1 , \"s\" : -1, \"d\" : 10, \"a\" : -10}\n",
        "    for i in control :\n",
        "        answer += dict_.get(i)\n",
        "    return answer\n",
        "\n",
        "n = 0\n",
        "control = \"wsdawsdassw\"\n",
        "solution(n, control)"
