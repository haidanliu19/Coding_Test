      "source": [
        "def solution(n, t):\n",
        "    answer = n\n",
        "    while t > 0:\n",
        "        answer *=  2  \n",
        "        t = t - 1\n",
        "    return answer\n",
        "\n",
        "n = 2\n",
        "t = 10\n",
        "solution(n, t)"
