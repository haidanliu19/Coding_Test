      "source": [
        "def solution(n):\n",
        "    answer = []\n",
        "    i = 2\n",
        "    while n > 1:\n",
        "        if n %i == 0:\n",
        "            answer.append(i)\n",
        "            n = n // i\n",
        "        else:\n",
        "            i += 1\n",
        "        \n",
        "    return sorted(list(set(answer)))\n",
        "\n",
        "n = 12\n",
        "solution(n)"
      ],
