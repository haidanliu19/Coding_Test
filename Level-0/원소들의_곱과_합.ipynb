      "source": [
        "def solution(num_list):\n",
        "    answer = 0\n",
        "    sum_squ = sum(num_list) **2\n",
        "    pro_ = 1\n",
        "    for i in num_list:\n",
        "        pro_ *= i\n",
        "    answer = 1 if pro_ < sum_squ else 0\n",
        "    return answer\n",
        "\n",
        "num_list = [3, 4, 5, 2, 1]\n",
        "solution(num_list)"
      ]
