        "def solution(myString, pat):\n",
        "    answer = 0\n",
        "\n",
        "    for i in range(len(myString)):\n",
        "        val = myString[i:][:len(pat)]\n",
        "        if val == pat:\n",
        "            answer += 1\n",
        "    return answer\n",
        "\n",
        "myString = \t\"aaaa\"\n",
        "pat = \"aa\"\n",
        "solution(myString, pat)"
