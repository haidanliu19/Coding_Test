      "source": [
        "def solution(arr, intervals):\n",
        "    answer = []\n",
        "    for i in intervals:\n",
        "        answer.extend(arr[i[0]: i[1] + 1])\n",
        "    return answer\n",
        "\n",
        "arr = [1, 2, 3, 4, 5]\n",
        "intervals = [[1, 3], [0, 4]]\n",
        "solution(arr, intervals)"
