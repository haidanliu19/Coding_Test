      "source": [
        "def solution(arr):\n",
        "    answer = [arr[0]]\n",
        "    for i in arr[1:]:\n",
        "        if (len(answer) > 0 ) and (i == answer[-1]):\n",
        "            answer.pop()\n",
        "        else:\n",
        "            answer.append(i)\n",
        "    return [-1] if len(answer) == 0 else answer\n",
        "\n",
        "arr = [0, 1, 1, 1, 0]\n",
        "solution(arr)"
      ]
