        "def solution(numlist, n):\n",
        "    answer = []\n",
        "    answer = [i[0] for i in sorted([(i, abs(i-n) )for i in numlist], key = lambda x: (x[1], -x[0]))]\n",
        "    return answer\n",
        "\n",
        "numlist = [1, 2, 3, 4, 5, 6]\n",
        "n = 4\n",
        "solution(numlist, n)"
