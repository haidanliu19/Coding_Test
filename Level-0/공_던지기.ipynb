      "source": [
        "def solution(numbers, k):\n",
        "    answer = []\n",
        "    for i in range(k):\n",
        "        answer.append(numbers[i * 2 % len(numbers)])\n",
        "    return answer[-1]\n",
        "\n",
        "numbers = [1, 2, 3, 4]\n",
        "k = 2\n",
        "solution(numbers, k)"
      ],
