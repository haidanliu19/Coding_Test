      "source": [
        "def solution(chicken):\n",
        "    answer = 0\n",
        "    while True:\n",
        "        div, mod = divmod(chicken, 10)\n",
        "        chicken= div +mod\n",
        "        answer += div\n",
        "        if div == 0:\n",
        "            break\n",
        "    return answer\n",
        "\n",
        "chicken = 1081\n",
        "solution(chicken)"
      ],
