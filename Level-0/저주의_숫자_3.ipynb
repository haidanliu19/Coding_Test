      "source": [
        "def solution(n):\n",
        "    answer = 0\n",
        "    result = []\n",
        "    cnt = 1\n",
        "    while len(result) < n :\n",
        "        if (cnt % 3 != 0) and str(cnt).count('3') < 1 :\n",
        "            result.append(cnt)\n",
        "        cnt += 1\n",
        "    answer = result[-1]\n",
        "    return answer\n",
        "\n",
        "n = 15\n",
        "solution(n)"
      ],
