      "source": [
        "def solution(num_list):\n",
        "    answer = []\n",
        "    answer.append(len(list(filter(lambda x :x % 2 ==0, num_list))))\n",
        "    answer.append(len(list(filter(lambda x :x % 2 !=0, num_list))))\n",
        "    return answer\n",
        "\n",
        "num_list = [1, 2, 3, 4, 5]\n",
        "solution(num_list)"
