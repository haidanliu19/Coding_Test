      "source": [
        "def solution(n):\n",
        "    answer = []\n",
        "    for i in range(1, n +1):\n",
        "        if i % 2 != 0:\n",
        "            answer.append(i)\n",
        "    return answer\n",
        "\n",
        "n = 10\n",
        "solution(n)"
