      "source": [
        "def solution(arr):\n",
        "    cnt = 0\n",
        "    len_arr = len(arr)\n",
        "    while 2 ** cnt < len_arr:\n",
        "        cnt += 1\n",
        "    answer = [arr[i] if i < len_arr else 0 for i in range(2 ** cnt )]\n",
        "    return answer\n",
        "\n",
        "\n",
        "arr = [0, 1, 2]\n",
        "solution(arr)"
      ]
