      "cell_type": "code",
      "source": [
        "def solution(n):\n",
        "    answer = 1\n",
        "    result = 1\n",
        "    while True:\n",
        "        result *= answer\n",
        "        if result > n:\n",
        "            answer -= 1\n",
        "            break\n",
        "        answer += 1\n",
        "    return answer\n",
        "\n",
        "n = 7\n",
        "solution(n)"
      ],
